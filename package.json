{
  "name": "vite-elm-template",
  "author": "Lindsay Wardell",
  "description": "A staring template for building Elm apps with Vite",
  "version": "1.1.2",
  "license": "MIT",
  "scripts": {
    "postinstall": "elm-tooling install",
    "dev": "npm run setem && vite",
    "prebuild": "elm-tooling install",
    "gen": "node generate.js && make setem",
    "build": "npm run setem && vite build",
    "serve": "vite preview",
    "test": "elm-test-rs",
    "setem": "setem --output gen/recordsetter"
  },
  "devDependencies": {
    "elm-format": "^0.8.6",
    "elm-test": "^0.19.1-revision7",
    "fs-extra": "^11.1.1",
    "level": "^8.0.0",
    "mustache": "^4.2.0",
    "setem": "^0.3.6",
    "vite": "^2.9.16",
    "vite-plugin-elm": "^2.7.2",
    "yaml": "^2.3.1"
  },
  "dependencies": {
    "elm-tooling": "^1.13.1",
    "file-saver": "^2.0.5",
    "file-select-dialog": "^1.5.4",
    "js-base64": "^3.7.2",
    "lzwcompress": "^1.1.0"
  }
}
